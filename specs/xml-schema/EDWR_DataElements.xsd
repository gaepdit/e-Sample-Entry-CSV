			<xsd:documentation>Name that indicates the method used</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="MethodDescriptionText" type="EN:MethodDescriptionTextDataType">
		<xsd:annotation>
			<xsd:documentation>A summary text describing the method used</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="MethodReferenceText" type="EN:MethodReferenceTextDataType">
		<xsd:annotation>
			<xsd:documentation>Bioliographic reference to the complete description of the methods</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="MethodDeviationsText" type="EN:MethodDeviationsTextDataType">
		<xsd:annotation>
