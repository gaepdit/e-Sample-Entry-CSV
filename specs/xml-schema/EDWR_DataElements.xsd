	<!-- End AnalyteIdentificationDataType -->
	<!-- Begin AuthenticationDataType -->
	<xsd:element name="CertificationIdentifier" type="EN:CertificationIdentifierDataType">
		<xsd:annotation>
			<xsd:documentation>Uniquely identifies the certified submission</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CertificationStatementText" type="EN:CertificationStatementTextDataType" default="I, the undersigned lead/chief operator of the water treatment plant listed in Part I of this form, certify that, to the best of my knowledge and belief, the information provided in this report is true and accurate. Also, I certify that the following additional operations records for the plant listed in Part I of this form were prepared each day that a certified operator staffed or visited the plant during the month indicated above: records of amounts of chemicals used and chemical feed rates; and appropriate treatment process performance records. Furthermore, I agree to retain these additional operations records at the plant site for at least five years and to make them available for review upon request.">
		<xsd:annotation>
			<xsd:documentation>The complete text of the certification statement</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CertifierName" type="EN:CertifierNameDataType">
		<xsd:annotation>
			<xsd:documentation>Identifies the certifier of the submission</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CertifierTitleText" type="EN:CertifierTitleTextDataType">
		<xsd:annotation>
			<xsd:documentation>Title of the individual certifying the submission</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CertificationDate" type="EN:CertificationDateDataType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>Certification date</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- End AuthenticationDataType -->
	<!-- Begin ChemicalSupplyDataType -->
	<xsd:element name="ChemicalName" type="EN:ChemicalNameDataType">
		<xsd:annotation>
			<xsd:documentation>Chlorine or Alum</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ChemicalSupplyWeight" type="EN:ChemicalSupplyWeightDataType">
		<xsd:annotation>
			<xsd:documentation>Chemical supply weight (lbs) on hand</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EstimatedSupplyDays" type="EN:EstimatedSupplyDaysDataType">
		<xsd:annotation>
			<xsd:documentation>Chemical Estimated Supply (days)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- End ChemicalSupplyDataType -->
	<!-- Begin CommentConstraint -->
	<xsd:element name="CommentType" type="EN:CommentTypeDataType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>Comment Type</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Comments" type="EN:CommentsDataType">
		<xsd:annotation>
			<xsd:documentation>Comment Type</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- Begin ContactPointDataType -->
	<!-- End ContactPointDataType -->
	<!-- Begin CrossConnectionDataType -->
	<xsd:element name="ProgramStartYear" type="EN:ProgramStartYearDataType">
		<xsd:annotation>
			<xsd:documentation>Year which local cross connection control program was formally started</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RoutineInspectionAccounts" type="EN:RoutineInspectionAccountsDataType">
		<xsd:annotation>
			<xsd:documentation>Total number of industrial, commercial, institutional, and miscellaneous accounts which must be routinely inspected for cross connections</xsd:documentation>
			<xsd:documentation>Total number of industrial, commercial, institutional, and miscellaneous accounts which must be routinely inspected for cross connections</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="TotalReportedInspections" type="EN:TotalReportedInspectionsDataType">
		<xsd:annotation>
			<xsd:documentation>Total number of accounts that were inspected in reporting year</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="TotalInspectionsSinceProgStart" type="EN:TotalInspectionsSinceProgStartDataType">
		<xsd:annotation>
			<xsd:documentation>Total number of accounts that have been inspected at least once since beginning the local cross connection control program</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CrossConnSinceProgStart" type="EN:CrossConnSinceProgStartDataType">
		<xsd:annotation>
			<xsd:documentation>Number of accounts from TotalInspectionsSinceProgramStart where a cross connection was found to exist</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CorrectiveActions" type="EN:CorrectiveActionsDataType">
		<xsd:annotation>
			<xsd:documentation>Number of accounts from CrossConnectionsSinceProgramStart where corrective actions have been completed</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AccountsInCompliance" type="EN:AccountsInComplianceDataType">
		<xsd:annotation>
			<xsd:documentation>Total number of accounts from the TotalInspectionsSinceProgramStart about which are now in compliance with the local cross connection control program</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RequiredReinspections" type="EN:RequiredReinspectionsDataType">
		<xsd:annotation>
			<xsd:documentation>Number of reinspections required each year based on approved frequency for reinspection</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ReportedReinspections" type="EN:ReportedReinspectionsDataType">
		<xsd:annotation>
			<xsd:documentation>Number of reinspections from RequiredReinspections being reported</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RequiredBackflowTesting" type="EN:RequiredBackflowTestingDataType">
		<xsd:annotation>
			<xsd:documentation>Total number of backflow prevention devices requiring testing</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ReportedBackflowTesting" type="EN:ReportedBackflowTestingDataType">
		<xsd:annotation>
			<xsd:documentation>Number of backflow prevention devices tested</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- End CrossConnectionDataType -->
	<!-- Begin CrossConnectionReportDataType -->
	<!-- End CrossConnectionReportDataType -->
	<!-- Begin DaliyDataResultDataType -->
	<xsd:element name="StatisticalBaseCode" type="EN:StatisticalBaseCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Statistical base code (e.g., Avg, Max, Min)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="MonitoringStartDate" type="EN:MonitoringStartDateDataType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>Monitoring starting date that applies to the daily data</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="MonitoringStartTime" type="EN:MonitoringStartTimeDataType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>Monitoring starting time that applies to the daily data</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="MonitoringEndDate" type="EN:MonitoringEndDateDataType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>Monitoring ending date that applies to the daily data</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="MonitoringEndTime" type="EN:MonitoringEndTimeDataType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>Monitoring ending time that applies to the daily data</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- Duplicate Element (used in AnalysisResultsDataType)
	<xsd:element name="ResultComment" type="EN:CommentType">
		<xsd:annotation>
			<xsd:documentation>Comment for each daily data value.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	-->
	<!-- End DaliyDataResultDataType -->
	<!-- Begin FluorideQualityAssuranceDataType -->
	<xsd:element name="QualityAssuranceSampleLocation" type="EN:QualityAssuranceSampleLocationDataType">
		<xsd:annotation>
			<xsd:documentation>Quality Assurance Sample Location</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="QualityAssuranceSampleNumber" type="EN:QualityAssuranceSampleNumberDataType">
		<xsd:annotation>
			<xsd:documentation>Quality Assurance Sample Number</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- End FluorideQualityAssuranceDataType -->
	<!-- Begin GeographicAreaDataType -->
	<xsd:element name="AdminRegion" type="EN:AdminRegionDataType">
		<xsd:annotation>
			<xsd:documentation>Enter the code value representing the state administrative region, if any, being served by a PWS in whole or in part.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AdminDistrict" type="EN:AdminDistrictDataType">
		<xsd:annotation>
			<xsd:documentation>Enter the code value representing the state administrative district, if any, being served by a PWS in whole or in part.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- End GeographicAreaDataType -->
	<!-- Begin IronAndManganeseSequestrantDataType -->
	<xsd:element name="SequestrantType" type="EN:SequestrantTypeDataType">
		<xsd:annotation>
			<xsd:documentation>Type of Sequestrant (polyphosphate or sodium silicate)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- End IronAndManganeseSequestrantDataType -->
	<!-- Begin LaboratoryAnalysisIdentificationDataType -->
	<xsd:element name="LabIdentifier" type="EN:LabIdentifierDataType">
		<xsd:annotation>
			<xsd:documentation>Identifier for the lab doing the analysis</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="LabAnalysisIdentifier" type="EN:LabAnalysisIdentifierDataType">
		<xsd:annotation>
			<xsd:documentation>A lab identifier for an analysis that uniquely identifies a single run for a single aliquot</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AnalysisContact" type="EN:AnalysisContactDataType">
		<xsd:annotation>
			<xsd:documentation>The name or code for the person to contact concerning information related to the analysis results</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="TestResultTypeCode" type="EN:TestResultTypeCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code for the type of test result being reported.  For example, analytical target, analytical surrogate, field result, derived data etc.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AnalysisBatchIdentifier" type="EN:AnalysisBatchIdentifierDataType">
		<xsd:annotation>
			<xsd:documentation>A lab defined identifier for a batch of analyses done on one instrument associated with the level of detail at which the instrument is checked to be in control.  If multiple analysis batches are used in a continuous sequence, this represents the start of any given analysis batch</xsd:documentation>
			<xsd:documentation>Laboratory certification number or Lab DOH</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AnalysisEquipmentUsed" type="EN:AnalysisEquipmentUsedDataType">
		<xsd:annotation>
			<xsd:documentation>Lab defined identifier or description of the instrument or equipment used for analysis</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AnalysisMatrix" type="EN:AnalysisMatrixDataType">
		<xsd:annotation>
			<xsd:documentation>Name, code or description of the sample matrix or media</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AnalysisStartDate" type="EN:AnalysisStartDateDataType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>The calendar date when the analysis began</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AnalysisStartTime" type="EN:AnalysisStartTimeDataType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>The local time and time zone when the analysis began</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AnalysisEndDate" type="EN:AnalysisEndDateDataType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>The calendar date (local time and time zone) when the analysis was finished</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AnalysisEndTime" type="EN:AnalysisEndTimeDataType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>The local time and time zone when the analysis was finished</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ExtractionDate" type="EN:ExtractionDateDataType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>The calendar date when extraction began</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ExtractionTime" type="EN:ExtractionTimeDataType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>The local time when extraction began</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- End LaboratoryAnalysisIdentificationDataType -->
	<!-- Begin LaboratoryIdentificationDataType -->
	<xsd:element name="LabName" type="EN:LabNameDataType">
		<xsd:annotation>
			<xsd:documentation>Name of the laboratory submitting the results or performing the analysis</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="LabTypeCode" type="EN:LabTypeCodeDataType">
		<xsd:annotation>
			<xsd:documentation>The classification of the lab</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SubContractedLabIndicator" type="EN:SubContractedLabIndicatorDataType">
		<xsd:annotation>
			<xsd:documentation>Indicates if all or some of the parameters were analyzed by a subcontracted lab</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- End LaboratoryIdentificationDataType -->
