			<xsd:documentation>Code indicating whether or not the analytical result meets established data quality criteria</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DataQualityRejectCode" type="EN:DataQualityRejectCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code representing the possible reason that the result may not meet data quality standards</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DataQualityRejectText" type="EN:DataQualityRejectTextDataType">
		<xsd:annotation>
			<xsd:documentation>Text describing the possible reason that the result may not meet data quality standards</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- End QAQCSummaryDataType-->
