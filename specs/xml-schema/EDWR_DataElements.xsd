		<xsd:annotation>
			<xsd:documentation>Water Level Static Value</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Pumping" type="EN:PumpingDataType">
		<xsd:annotation>
			<xsd:documentation>Water Level Pumping Value</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- End PumpageDataDataType -->
	<!-- Begin PumpageReportDataType-->
	<xsd:element name="MaximumPumpageDate" type="EN:MaximumPumpageDateDataType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>Date of Maximum Pumpage</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="MinimumPumpageDate" type="EN:MinimumPumpageDateDataType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>Date of Minimum Pumpage</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
