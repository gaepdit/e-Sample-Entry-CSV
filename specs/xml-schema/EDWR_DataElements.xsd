	<!-- Begin PWSCharacteristicsDataType-->
	<xsd:element name="ActivityIndicator" type="EN:ActivityIndicatorDataType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>Enter an “A” if the PWS is active or an “I” if the water system is inactive</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DeactivationDate" type="EN:DeactivationDateDataType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>Enter the date PWS was deactivated</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SurfacePercent" type="EN:SurfacePercentDataType">
		<xsd:annotation>
			<xsd:documentation>Annual average percentage of water obtained from non purchased, permanently available surface water sources.  The total of this percentage along with GroundPercent, PurchasedSurfacePrecent, and PurchasedGroundPercent should be equal to 100</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="GroundPercent" type="EN:GroundPercentDataType">
		<xsd:annotation>
			<xsd:documentation>Annual average percentage of water obtained from non purchased, permanently available ground water sources. The total of this percentage along with SurfacePercent, PurchasedSurfacePrecent, and PurchasedGroundPercent should be equal to 100</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PurchasedSurfacePercent" type="EN:PurchasedSurfacePercentDataType">
		<xsd:annotation>
			<xsd:documentation>Annual average percentage of water obtained from purchased, permanently available surface water sources. The total of this percentage along with GroundPercent, SurfacePrecent, and PurchasedGroundPercent should be equal to 100</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PurchasedGroundPercent" type="EN:PurchasedGroundPercentDataType">
		<xsd:annotation>
			<xsd:documentation>Annual average percentage of water obtained from purchased, permanently available ground water sources.  The total of this percentage along with GroundPercent, SurfacePercent, and PurchasedSurfacePrecent should be equal to 100</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AverageDailyProductionValue" type="EN:AverageDailyProductionValueDataType">
		<xsd:annotation>
			<xsd:documentation>Number of gallons representing the average daily quantity of water produced by a public water system in the most recent year.  This may be calculated by adding together the total gallons of water produced in a year by each treatment or pumping facility, and dividing that sum by the total number of days that at least one treatment or pumping facility was in operation</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DesignCapacityValue" type="EN:DesignCapacityValueDataType">
		<xsd:annotation>
			<xsd:documentation>Number of gallons per day representing the total quantity of water a public water system was designed and approved to produce.  This may be calculated by adding together the approved design capacity of each treatment or pumping facility</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EmergencyProductionMeasure" type="EN:EmergencyProductionMeasureDataType">
		<xsd:annotation>
			<xsd:documentation>Number of gallons per day representing the total quantity of water that can be produced by a public water system using emergency power generation equipment under its control.  This may be calculated by adding together the emergency power production capacity of each treatment or pumping facility</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="StorageCapacityMeasure" type="EN:StorageCapacityMeasureDataType">
		<xsd:annotation>
			<xsd:documentation>Number of gallons representing the total quantity of water that can be stored by a public water system.  This may be calculated by adding together the storage capacity of each water storage facility</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="NonCommunitySeasonBegin" type="EN:NonCommunitySeasonBeginDataType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>Month and day a non community or non transient non community water system’s season of operation normally begins</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="NonCommunitySeasonEnd" type="EN:NonCommunitySeasonEndDataType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>Month and day a non community or non transient non community water system’s season of operation normally ends</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- End PWSCharacteristicsDataType-->
