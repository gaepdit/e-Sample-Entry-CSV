	<!-- End LabReportDataType -->
	<!-- Begin MeasurementDataType -->
	<xsd:element name="MeasurementQualifier">
		<xsd:annotation>
			<xsd:documentation>The qualifier for the measurement value, for example: less than, greater than, ND, etc. </xsd:documentation>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="EN:MeasurementQualifierDataType">
				<xsd:minLength value="0"/>
				<xsd:maxLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MeasurementValue">
		<xsd:annotation>
			<xsd:documentation>A number, code, or text to identify the measurement value</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="EN:MeasurementValueDataType">
				<xsd:maxInclusive value="9999999999"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MeasurementUnit">
		<xsd:annotation>
			<xsd:documentation>The unit of measure for the measurement value</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="EN:MeasurementUnitDataType">
				<xsd:minLength value="0"/>
				<xsd:maxLength value="9"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MeasurementSignificantDigit" type="EN:MeasurementSignificantDigitDataType">
		<xsd:annotation>
			<xsd:documentation>The number of significant digits for the measurement value</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- End MeasurementDataType -->
	<!-- Begin MetaDataDataType -->
	<xsd:element name="SchemaIdentificationText" type="EN:SchemaIdentificationTextDataType" default="e-DWR Schema Version 2.2">
		<xsd:annotation>
			<xsd:documentation>Identification of the schema</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SchemaDescriptionText" type="EN:SchemaDescriptionTextDataType" default="e-DWR Work Group Schema version 2.2">
		<xsd:annotation>
			<xsd:documentation>General description of the schema</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SchemaPurposeText" type="EN:SchemaPurposeTextDataType" default="Electronic Drinking Water Report submissions">
		<xsd:annotation>
			<xsd:documentation>Purpose of the schema</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SchemaVersionIdentifier" type="EN:SchemaVersionIdentifierDataType" default="2.2">
