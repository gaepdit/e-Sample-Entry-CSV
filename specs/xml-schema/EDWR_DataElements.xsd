			<xsd:restriction base="EN:MethodIdentifierDataType">
				<xsd:minLength value="0"/>
				<xsd:maxLength value="30"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MethodName" type="EN:MethodNameDataType">
