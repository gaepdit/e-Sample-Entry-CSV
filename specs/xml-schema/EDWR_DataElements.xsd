	<xsd:element name="BatchQCType" type="EN:BatchQCTypeDataType">
		<xsd:annotation>
			<xsd:documentation>Name or code for QC sample type related to a batch of samples</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AnalyticalPrecision" type="EN:AnalyticalPrecisionDataType">
		<xsd:annotation>
			<xsd:documentation>Measure of the degree of agreement among replicate analyses of a sample, usually expressed as the standard deviation</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AnalyticalBias" type="EN:AnalyticalBiasDataType">
		<xsd:annotation>
			<xsd:documentation>Bias (accuracy) of an analytical procedure, which reflects the closeness of a measured value to a true value</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="UpperControlLimit" type="EN:UpperControlLimitDataType">
		<xsd:annotation>
			<xsd:documentation>Value of upper end of acceptable range</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="LowerControlLimit" type="EN:LowerControlLimitDataType">
		<xsd:annotation>
			<xsd:documentation>Value of lower end of acceptable range</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
