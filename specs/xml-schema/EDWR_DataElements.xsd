	<xsd:element name="QAOfficerName" type="EN:QAOfficerNameDataType">
		<xsd:annotation>
			<xsd:documentation>Full name of the Quality Assurance Officer</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="LabManagerName" type="EN:LabManagerNameDataType">
		<xsd:annotation>
			<xsd:documentation>Full name of the Laboratory Manager</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!--Duplicate Element
	<xsd:element name="LabAnalysisIdentification" type="EN:LaboratoryAnalysisIdentificationDataType">
