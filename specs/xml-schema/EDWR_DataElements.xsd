			<xsd:documentation>Version of the schema</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SchemaCreatedByName" type="EN:SchemaCreatedByNameDataType" default="Lab to State Drinking Water IPT">
		<xsd:annotation>
			<xsd:documentation>Schema created by</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SchemaCreatedDate" type="EN:SchemaCreatedDateDataType" default="2002-04-12" nillable="true">
		<xsd:annotation>
			<xsd:documentation>Schema created date</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SchemaLastUpdatedByName" type="EN:SchemaLastUpdatedByNameDataType" default="Lab to State Drinking Water IPT">
