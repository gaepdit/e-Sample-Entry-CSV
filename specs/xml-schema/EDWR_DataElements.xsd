	<xsd:element name="Day" type="EN:DayDataType">
		<xsd:annotation>
			<xsd:documentation>Pumpage/Usage Month</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Date" type="EN:DateDataType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>Date Water Level was taken</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
